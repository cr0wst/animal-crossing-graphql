    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.8.1",
